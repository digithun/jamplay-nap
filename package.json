{
  "name": "nap",
  "version": "1.28.3",
  "main": "index.js",
  "standard": {
    "globals": [
      "it",
      "describe",
      "expect"
    ]
  },
  "scripts": {
    "postinstall": "cd graphql/content && npm install || true",
    "setup": "echo \"# See .env.example for more\" > .env",
    "lint": "eslint . || true",
    "_dev": "next",
    "_prod": "NODE_ENV=production npm run _build && npm run _serve",
    "_build": "next build",
    "_start": "next start",
    "_serve": "NODE_ENV=production node index.js",
    "_serve-dev": "nodemon --verbose --inspect=0.0.0.0:5858 index.js",
    "_build-front": "next build && next start",
    "_build-back": "next build && npm run _serve",
    "_build-back-dev": "next build && npm run _serve-dev",
    "dev": "npm run exec -- npm run _dev",
    "build": "npm run exec -- npm run _build",
    "start": "npm run exec -- npm run _start",
    "serve": "npm run exec -- npm run _serve",
    "prod": "npm run exec -- npm run _prod",
    "local": "npm run stop && REDIS_URI=redis://localhost MONGODB_URI=mongodb://localhost/graphql npm run _serve",
    "build:image": "sh ./scripts/build-image.bash",
    "image-dev": "NODE_ENV=development docker build --no-cache -t digithun/jamplay-nap . || true",
    "history": "docker history digithun/jamplay-nap > history.log && cat history.log || true",
    "size": "echo $(docker image inspect digithun/jamplay-nap -f {{.Size}})",
    "in": "docker exec -it $(docker ps -a -q --filter ancestor=digithun/jamplay-nap) /bin/sh || true",
    "exec": "docker exec -it $(docker ps -a -q --filter ancestor=digithun/jamplay-nap)",
    "rmi": "docker rmi -f digithun/jamplay-nap || true",
    "clean": "docker rm -f $(docker ps -a -q --filter ancestor=digithun/jamplay-nap) || true",
    "run": "docker run -p 3000:3000 -p 3001:3001 -d digithun/jamplay-nap || true",
    "run-dev": "docker run -p 3000:3000 -p 3001:3001 -p 5858:5858 -p 9229:9229 -d digithun/jamplay-nap || true",
    "logs": "docker logs $(docker ps -a -q --filter ancestor=digithun/jamplay-nap) || true",
    "stop": "docker stop $(docker ps -a -q --filter ancestor=digithun/jamplay-nap) || true",
    "clear": "docker rm $(docker ps -aq) -f || true && docker rmi -f digithun/jamplay-nap && docker system prune -f && docker images && docker ps -a || true",
    "down": "docker-compose kill || true && docker-compose down || true",
    "up": "docker-compose build && docker-compose up -d && docker ps || true",
    "up-dev": "NODE_ENV=development docker-compose -f docker-compose.yml -f docker-compose.dev.yml build && docker-compose -f docker-compose.yml -f docker-compose.dev.yml up -d && docker ps -a || true && npm run attach",
    "attach": "docker attach $(docker ps -a -q --filter ancestor=digithun/jamplay-nap) || true",
    "graphiql": "docker run -it -p3002:3000 vitalcode/auth-graphiql",
    "test": "NODE_ENV=test jest --watchAll --runInBand",
    "test:once": "NODE_ENV=test jest --runInBand",
    "cover": "NODE_ENV=test jest --coverage --no-cache",
    "release": "standard-version --release",
    "release-patch": "standard-version --release-as patch",
    "release-rc": "standard-version --prerelease rc",
    "migrate": "node -r dotenv/config ./graphql/content/scripts/migrate"
  },
  "dependencies": {
    "@types/draft-js": "^0.10.18",
    "apollo-client": "1.9.3",
    "apollo-fetch": "^0.6.0",
    "apollo-server-express": "^1.2.0",
    "apollo-upload-server": "^2.0.4",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "chalk": "^2.3.0",
    "connect-redis": "^3.3.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "cron": "^1.3.0",
    "dataloader": "^1.3.0",
    "dotenv": "^4.0.0",
    "draft-js": "^0.10.4",
    "es6-promise": "^4.1.1",
    "express": "^4.16.2",
    "express-bearer-token": "^2.1.0",
    "express-graphql": "^0.6.11",
    "express-ping": "^1.4.0",
    "express-session": "^1.15.6",
    "express-winston": "^2.4.0",
    "graphql": "^0.11.7",
    "graphql-comment": "^0.0.8",
    "graphql-compose": "1.21.1",
    "graphql-compose-connection": "2.3.0",
    "graphql-compose-mongoose": "1.8.0",
    "graphql-compose-recompose": "0.0.1",
    "graphql-compose-relay": "^1.1.1",
    "graphql-tag": "^2.5.0",
    "graphql-tools": "^2.6.1",
    "helmet": "^3.9.0",
    "imageinfo": "^1.0.4",
    "immutable": "^3.8.2",
    "is-progressive": "^3.0.0",
    "is_js": "^0.9.0",
    "isomorphic-fetch": "^2.2.1",
    "jsonwebtoken": "^8.1.0",
    "left-pad": "^1.1.3",
    "localforage": "^1.5.2",
    "lusca": "^1.5.2",
    "mailgun.js": "^2.0.1",
    "md5": "^2.2.1",
    "mongoose": "^4.12.4",
    "mongoose-delete": "^0.4.0",
    "mongoose-role": "^2.0.1",
    "mubsub": "^1.4.0",
    "next": "^3.2.2",
    "node-monitor-ui": "^1.0.6",
    "optics-agent": "^1.1.8",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-facebook-token": "^3.3.0",
    "passport-github2": "^0.1.11",
    "passport-google-oauth": "^1.0.0",
    "passport-local": "^1.0.0",
    "passport-twitter": "^1.0.4",
    "path-match": "^1.2.4",
    "platform": "^1.3.4",
    "promise-polyfill": "^6.0.2",
    "react": "^15.6.1",
    "react-apollo": "^1.4.15",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "redis": "^2.8.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "rimraf": "^2.6.2",
    "sharp": "^0.18.4",
    "superagent": "^3.7.0",
    "twitter": "^1.7.1",
    "uuid": "^3.1.0",
    "validator": "^9.1.1",
    "winston": "^2.4.0",
    "winston-daily-rotate-file": "^1.7.2"
  },
  "author": "katopz@gmail.com",
  "license": "MIT",
  "repository": "https://github.com/digithun/jamplay-nap",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-register": "^6.26.0",
    "caporal": "^0.8.0",
    "casual": "^1.5.17",
    "docker-build-run-push": "^3.0.0",
    "enzyme": "^3.1.0",
    "eslint": "^4.9.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-react": "^7.4.0",
    "is-progressive": "^3.0.0",
    "jest": "^21.2.1",
    "jest-cli": "^21.2.1",
    "mongodb-memory-server": "^1.6.1",
    "nodemon": "^1.12.1",
    "promise-sequential": "^1.1.1",
    "prop-types": "^15.6.0",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.0.0",
    "standard-version": "^4.2.0"
  }
}
