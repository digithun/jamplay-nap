{
  "name": "nap",
  "version": "0.34.0",
  "main": "index.js",
  "scripts": {
    "postinstall": "cd graphql/content && yarn install",
    "setup": "echo \"# See .env.example for more\" > .env",
    "lint": "eslint . || true",
    "_dev": "next",
    "_prod": "NODE_ENV=production npm run _build && npm run _serve",
    "_build": "next build",
    "_start": "next start",
    "_serve": "NODE_ENV=production node index.js",
    "_serve-dev": "nodemon --verbose --inspect=0.0.0.0:5858 index.js",
    "_build-front": "next build && next start",
    "_build-back": "next build && npm run _serve",
    "_build-back-dev": "next build && npm run _serve-dev",
    "dev": "npm run exec -- npm run _dev",
    "build": "npm run exec -- npm run _build",
    "start": "npm run exec -- npm run _start",
    "serve": "npm run exec -- npm run _serve",
    "prod": "npm run exec -- npm run _prod",
    "local": "npm run stop && REDIS_URI=redis://localhost MONGODB_URI=mongodb://localhost/graphql npm run _serve",
    "docker:build-alpha": "docker build -f Dockerfile.jamplay -t reg.jamplay.world/jamplay/jamplay-nap-alpha:$(node -p \"require('./package.json').version\") . && docker tag reg.jamplay.world/jamplay/jamplay-nap-alpha:$(node -p \"require('./package.json').version\") reg.jamplay.world/jamplay/jamplay-nap-alpha:latest",
    "docker:build-beta": "docker build -f Dockerfile.jamplay -t reg.jamplay.world/jamplay/jamplay-nap-beta:$(node -p \"require('./package.json').version\") . && docker tag reg.jamplay.world/jamplay/jamplay-nap-beta:$(node -p \"require('./package.json').version\") reg.jamplay.world/jamplay/jamplay-nap-beta:latest",
    "docker:build-local": "docker build -f Dockerfile.jamplay -t reg.jamplay.world/jamplay/jamplay-nap-local:$(node -p \"require('./package.json').version\") . && docker tag reg.jamplay.world/jamplay/jamplay-nap-local:$(node -p \"require('./package.json').version\") reg.jamplay.world/jamplay/jamplay-nap-local:latest",
    "docker:push-image-alpha": "docker push reg.jamplay.world/jamplay/jamplay-nap-alpha",
    "docker:push-image-beta": "docker push reg.jamplay.world/jamplay/jamplay-nap-beta",
    "docker:push-image-local": "docker push reg.jamplay.world/jamplay/jamplay-nap-local",
    "build:image": "docker build --no-cache -t digithun/jamplay-nap:$(node -p \"require('./package.json').version\") -f Dockerfile.jamplay . && docker tag digithun/jamplay-nap:$(node -p \"require('./package.json').version\") digithun/jamplay-nap:latest || true",
    "image-dev": "NODE_ENV=development docker build --no-cache -t digithun/jamplay-nap . || true",
    "history": "docker history digithun/jamplay-nap > history.log && cat history.log || true",
    "size": "echo $(docker image inspect digithun/jamplay-nap -f {{.Size}})",
    "in": "docker exec -it $(docker ps -a -q --filter ancestor=digithun/jamplay-nap) /bin/sh || true",
    "exec": "docker exec -it $(docker ps -a -q --filter ancestor=digithun/jamplay-nap)",
    "rmi": "docker rmi -f digithun/jamplay-nap || true",
    "clean": "docker rm -f $(docker ps -a -q --filter ancestor=digithun/jamplay-nap) || true",
    "run": "docker run -p 3000:3000 -d digithun/jamplay-nap || true",
    "run-dev": "docker run -p 3000:3000 -p 5858:5858 -p 9229:9229 -d digithun/jamplay-nap || true",
    "logs": "docker logs $(docker ps -a -q --filter ancestor=digithun/jamplay-nap) || true",
    "stop": "docker stop $(docker ps -a -q --filter ancestor=digithun/jamplay-nap) || true",
    "clear": "docker rm $(docker ps -aq) -f || true && docker rmi -f digithun/jamplay-nap && docker system prune -f && docker images && docker ps -a || true",
    "down": "docker-compose kill || true && docker-compose down || true",
    "up": "docker-compose build && docker-compose up -d && docker ps || true",
    "up-dev": "NODE_ENV=development docker-compose -f docker-compose.yml -f docker-compose.dev.yml build && docker-compose -f docker-compose.yml -f docker-compose.dev.yml up -d && docker ps -a || true && npm run attach",
    "attach": "docker attach $(docker ps -a -q --filter ancestor=digithun/jamplay-nap) || true",
    "graphiql": "docker run -it -p3001:3000 vitalcode/auth-graphiql",
    "test": "jest --watchAll --runInBand",
    "test:once": "jest --runInBand",
    "cover": "jest --coverage --no-cache",
    "release": ". release.sh",
    "seed": "node ./graphql/content/seed.js",
    "gen-seed": "node ./scripts/seeds/gen-seed.js",
    "import-seed": "node ./scripts/seeds/import.js",
    "remove-seed": "node ./scripts/seeds/remove.js",
    "re-gen-seed": "npm run gen-seed && npm run remove-seed && npm run import-seed",
    "seed-episode-binary": "node ./scripts/seeds/seed-episode-binary.js"
  },
  "dependencies": {
    "apollo-client": "1.9.2",
    "apollo-upload-server": "^2.0.4",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.1",
    "connect-redis": "^3.3.0",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "es6-promise": "^4.1.1",
    "express": "^4.15.4",
    "express-bearer-token": "^2.1.0",
    "express-graphql": "^0.6.11",
    "express-session": "^1.15.5",
    "graphql": "^0.11.3",
    "graphql-comment": "^0.0.8",
    "graphql-compose": "1.21.1",
    "graphql-compose-connection": "^2.3.0",
    "graphql-compose-mongoose": "1.8.0",
    "graphql-compose-recompose": "0.0.1",
    "graphql-compose-relay": "^1.1.0",
    "graphql-tag": "^2.4.2",
    "graphql-tools": "^1.2.2",
    "helmet": "^3.8.1",
    "imageinfo": "^1.0.4",
    "is_js": "^0.9.0",
    "isomorphic-fetch": "^2.2.1",
    "jsonwebtoken": "^8.0.1",
    "left-pad": "^1.1.3",
    "localforage": "^1.5.0",
    "lusca": "^1.5.1",
    "mailgun.js": "^2.0.1",
    "mongoose": "^4.11.11",
    "mongoose-delete": "^0.4.0",
    "mongoose-role": "^2.0.1",
    "mubsub": "^1.4.0",
    "multer": "^1.3.0",
    "next": "^3.2.2",
    "optics-agent": "^1.1.6",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-facebook-token": "^3.3.0",
    "passport-github2": "^0.1.10",
    "passport-google-oauth": "^1.0.0",
    "passport-local": "^1.0.0",
    "passport-twitter": "^1.0.4",
    "path-match": "^1.2.4",
    "platform": "^1.3.4",
    "promise-polyfill": "^6.0.2",
    "react": "^15.6.1",
    "react-apollo": "^1.4.15",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "redis": "^2.8.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "sharp": "^0.18.3",
    "tmp-promise": "^1.0.3",
    "uuid": "^3.1.0",
    "validator": "^8.2.0"
  },
  "author": "katopz@gmail.com",
  "license": "MIT",
  "repository": "https://github.com/digithun/jamplay-nap",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-jest": "^21.0.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-register": "^6.26.0",
    "caporal": "^0.7.0",
    "casual": "^1.5.17",
    "docker-build-run-push": "^3.0.0",
    "enzyme": "^2.9.1",
    "eslint": "^4.6.1",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-react": "^7.3.0",
    "jest": "^21.1.0",
    "jest-cli": "^21.1.0",
    "mongodb-memory-server": "^1.6.0",
    "nodemon": "^1.12.1",
    "promise-sequential": "^1.1.1",
    "prop-types": "^15.5.10",
    "react-addons-test-utils": "^15.6.0",
    "react-test-renderer": "^15.6.1",
    "superagent": "^3.6.0"
  }
}
